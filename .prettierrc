{
  "importOrder": [
    "<TYPES>",
    "<TYPES>^@/(.*)$",
    "",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/theme(.*)$",
    "^@/utils(.*)$",
    "",
    "^@/components(.*)$",
    "^@/screens(.*)$",
    "",
    "^@/(.*)$",
    "",
    "^[.]"
